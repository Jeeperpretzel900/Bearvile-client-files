		var versionClient = "Workshop.swf";
		var so = new SWFObject(versionClient, "Workshop", "750", "510", "8,0,0,0", "#000000");
		so.addVariable("section", GetParam("section"));
		so.write("flashcontent");
	</script>	
</div>
