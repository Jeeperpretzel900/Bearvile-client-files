	<transportdevice type_id="17">
		<!--
		Each transport device in this section is described with 6 parameters :
			id : unic identifier of the package (no one should be the same)
			type_id : identifier of the package type  (should be 17)
			lang_id : identifier of the package section in the language xml file(should be 17)
			path : flash shockwave file that contains the item's graphic
			name_id : identifier of the package's name in the language xml file (usualy even)
					points to the id parameter
			desc_id : identifier of the package's description in the language xml file (usualy odd)
					points to the id parameter
			equiv : identifier of the frame (minus 1) where the graphics are integrated
			device_number : identifier of the transportation device type (1 = Hoverboard, 2 = propelled hat, 3= segway, ...)
					used to find the right animation to play with the transportation device
			body_part : identifier of the avatar's concerned part (always 0)
					this is used because transportdevice is called whit the item and a bodypart is necessary.
		-->
		<item id="0" type_id="17" lang_id="17" path="TransportDevice1.swf" name_id="0" desc_id="1" equiv="1" device_number="1" body_part="0" />
		<item id="1" type_id="17" lang_id="17" path="TransportDevice2.swf" name_id="2" desc_id="3" equiv="2" device_number="2" body_part="0" />
		<item id="2" type_id="17" lang_id="17" path="TransportDevice3.swf" name_id="4" desc_id="5" equiv="2" device_number="3" body_part="0" />
	</transportdevice>
	
</item>